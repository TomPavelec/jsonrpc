{
  "name": "contributte/jsonrpc",
  "description": "JSON-RPC toolset build on top of psr-7 (guzzlehttp), league/json-guard, league/json-reference, league/flysystem and nette/di",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Janda",
      "email": "me@paveljanda.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Contributte\\JsonRPC\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Contributte\\JsonRPC\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.1.11",
    "nette/utils": "^4.0.3",
    "nette/bootstrap": "^3.2.1",
    "guzzlehttp/guzzle": "^7.8.0",
    "psr/http-message": "^1.1.0",
    "justinrainbow/json-schema": "^5.2.11",
    "league/flysystem": "^1.1.10",
    "psr/cache": "^2.0 || ^3.0"
  },
  "require-dev": {
    "nette/tester": "^2.4.1",
    "mockery/mockery": "^1.4",
    "predis/predis": "^1.1",
    "contributte/code-rules": "^1.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
